{
  "name": "commerce-studio-consultation",
  "version": "1.0.0",
  "description": "Intelligent eyewear consultation system for Commerce Studio",
  "type": "module",
  "main": "scripts/start-consultation-system.js",
  "scripts": {
    "start": "node scripts/start-consultation-system.js",
    "start:test": "node scripts/start-consultation-system.js start --test",
    "test": "node scripts/start-consultation-system.js test",
    "health": "node scripts/start-consultation-system.js health",
    "dev": "nodemon scripts/start-consultation-system.js",
    "serve:html": "cd apps/html-store && python3 -m http.server 3001",
    "serve:html:win": "cd apps/html-store && python -m http.server 3001",
    "setup": "npm install && node scripts/setup-environment.sh",
    "validate": "node scripts/validate-security.sh"
  },
  "keywords": [
    "eyewear",
    "consultation",
    "ai",
    "dialogflow",
    "virtual-try-on",
    "ecommerce"
  ],
  "author": "Commerce Studio Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/dialogflow-cx": "^4.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.8",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commerce-studio/consultation-system.git"
  },
  "bugs": {
    "url": "https://github.com/commerce-studio/consultation-system/issues"
  },
  "homepage": "https://github.com/commerce-studio/consultation-system#readme"
}
